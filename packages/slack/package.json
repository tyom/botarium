{
  "name": "@botarium/slack",
  "version": "0.1.0",
  "description": "Slack platform plugin for Botarium",
  "type": "module",
  "license": "MIT",
  "author": "Tyom Semonov",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyom/botarium",
    "directory": "packages/slack"
  },
  "keywords": [
    "slack",
    "bot",
    "emulator",
    "simulator",
    "botarium"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "emulator": "bun run src/server/index.ts",
    "emulator:shared": "bun --env-file=../../.env.shared run src/server/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "pino": "^10.1.0"
  },
  "devDependencies": {
    "pino-pretty": "^13.1.3"
  },
  "peerDependencies": {
    "botarium": "workspace:*"
  }
}
