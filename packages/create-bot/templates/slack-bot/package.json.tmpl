{
  "name": "{{packageName}}",
  "version": "0.1.0",
  "description": "{{botName}} Slack bot",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/app.ts",
    "dev": "bun --watch run src/app.ts",
    "dev:local": "SLACK_API_URL=http://localhost:7557 bun --watch run src/app.ts",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "@slack/bolt": "^4.6.0",
    "@slack/types": "^2.15.0",
    "@slack/web-api": "^7.10.0",
    "pino": "^10.1.0",
    "zod": "^4.3.5"
    {{~#if isAi~}}
    ,
    "ai": "^4.3.16"
    {{~/if}}
    {{~#if isOpenai~}}
    ,
    "@ai-sdk/openai": "^1.3.22"
    {{~/if}}
    {{~#if isAnthropic~}}
    ,
    "@ai-sdk/anthropic": "^1.2.12"
    {{~/if}}
    {{~#if isGoogle~}}
    ,
    "@ai-sdk/google": "^1.2.20"
    {{~/if}}
  },
  "devDependencies": {
    "@types/bun": "latest",
    "pino-pretty": "^13.1.3",
    "typescript": "^5"
  }
