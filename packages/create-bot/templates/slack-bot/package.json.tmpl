{
  "name": "{{packageName}}",
  "version": "0.1.0",
  "description": "{{botName}} Slack bot",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "dev:local": "SLACK_API_URL=http://localhost:7557 bun --watch run src/index.ts",
    "typecheck": "tsc --noEmit"
    {{~#if isDb~}}
    ,
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
    {{~/if}}
  },
  "dependencies": {
    "@slack/bolt": "^4.6.0",
    "@slack/types": "^2.15.0",
    "@slack/web-api": "^7.10.0",
    "pino": "^10.1.0",
    "zod": "^4.3.5"
    {{~#if isAi~}}
    ,
    "ai": "^6.0.13"
    {{~/if}}
    {{~#if isOpenai~}}
    ,
    "@ai-sdk/openai": "^3.0.6"
    {{~/if}}
    {{~#if isAnthropic~}}
    ,
    "@ai-sdk/anthropic": "^3.0.7"
    {{~/if}}
    {{~#if isGoogle~}}
    ,
    "@ai-sdk/google": "^3.0.5"
    {{~/if}}
    {{~#if isDb~}}
    ,
    "drizzle-orm": "^0.45.1"
    {{~/if}}
    {{~#if isPostgres~}}
    ,
    "postgres": "^3.4.8"
    {{~/if}}
  },
  "devDependencies": {
    "@types/bun": "latest",
    "pino-pretty": "^13.1.3",
    "typescript": "^5"
    {{~#if isDb~}}
    ,
    "drizzle-kit": "^0.31.8"
    {{~/if}}
  }
}
